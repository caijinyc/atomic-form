{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react",
    "module": "esnext",
    "target": "es2016",
    "lib": ["esnext", "dom"],
    "allowJs": false,
    "noUnusedLocals": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": false,
    "declaration": true,
    "experimentalDecorators": true,
    "downlevelIteration": true
  }
}

